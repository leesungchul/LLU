<!DOCTYPE html>
<html>
<head>
  <title>LLU</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag "//netdna.bootstrapcdn.com/bootswatch/3.0.3/cerulean/bootstrap.min.css", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash.now[:errors] %>
    <% flash.now[:errors].each do |error| %>
     <%= error %><br />
    <% end %>
  <% end %>
  <%= render "layouts/form" %>
  <%= render "layouts/navbar" %>
  <%= yield %>

</body>
</html>
